<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
http://java.sun.com/xml/ns/javaee/web-app_2_4.xsd"
  version="2.4">

	<servlet>
		<servlet-name>inputtest</servlet-name>
		<jsp-file>/input.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>inputtest</servlet-name>
		<url-pattern>/inputtest</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>registtest</servlet-name>
		<servlet-class>RegistTestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>registtest</servlet-name>
		<url-pattern>/registtest</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>registresult</servlet-name>
		<jsp-file>/registresult.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>registresult</servlet-name>
		<url-pattern>/registresult</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>practice</servlet-name>
		<jsp-file>/practice.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>practice</servlet-name>
		<url-pattern>/pr</url-pattern>
	</servlet-mapping>
	
	<error-page>
		<exception-type>javax.servlet.ServletException</exception-type>
		<location>/nocontent.jsp</location>
	</error-page>
	
</web-app>